<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ellisallmond.payrolltracker">

    <uses-permission-sdk-23 android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission-sdk-23 android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".PayWeek"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewPayActivity"
            android:label="@string/title_activity_new_pay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ExistingPayWeekActivity"
            android:label="@string/title_activity_existing_pay_week"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SetRates"
            android:label="@string/title_activity_set_rates"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>